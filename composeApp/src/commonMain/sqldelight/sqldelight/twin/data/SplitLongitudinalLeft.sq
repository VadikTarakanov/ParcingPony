CREATE TABLE LongitundinalSplitProgressLeft (
 id INTEGER PRIMARY KEY AUTOINCREMENT,
 progress INTEGER,
 date_training TEXT DEFAULT (datetime('now')) NOT NULL,
 type_training TEXT NOT NULL
);
selectAll:
SELECT * FROM LongitundinalSplitProgressLeft;

insertItem:
INSERT INTO LongitundinalSplitProgressLeft (progress, date_training, type_training) VALUES (:progress, :date_training, :type_training);

deleteItem:
DELETE FROM LongitundinalSplitProgressLeft WHERE id = :id;